<dashboard showsource="true" script="autodiscover.js">
  <label>Exemple</label>
  <search id="table">
    <query>| inputlookup exemples.csv |join max=0 type=outer [|inputlookup columns.csv |rename type as col_type parent as parent_col]|table columns rows col_type parent_col count|sort rows|</query>
    <earliest>0</earliest>
    <latest></latest>
  </search>

  <row>
    <panel>
      <table>
        <title>Table source</title>
        <search base="table"></search>
      </table>
    </panel>
    <panel>
      <viz type="viz_extensions.viz1">
        <title>Table Viz</title>
        <search base="table"></search>
        <option name="height">400</option>
        <option name="viz_extensions.viz1.Color1">#FFCCCC</option>
        <option name="viz_extensions.viz1.Color2">#DED9D9</option>
        <option name="viz_extensions.viz1.Color3">#B2AFAF</option>
        <option name="viz_extensions.viz1.Color4">#7C7A7A</option>
        <option name="viz_extensions.viz1.Color5">#000000</option>
        <option name="viz_extensions.viz1.case_font_size">34</option>
        <option name="viz_extensions.viz1.case_size">80</option>
        <option name="viz_extensions.viz1.scale">0.6</option>
        <option name="viz_extensions.viz1.x_title">X Title</option>
        <option name="viz_extensions.viz1.y_title">Y Title</option>
      </viz>
    </panel>
  </row>
</dashboard>
